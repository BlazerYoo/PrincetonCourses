<%

if (typeof (release) === 'undefined') {
  var release = 'v0'
}
%>
<script>releaseVersion = '<%= release %>';</script>
<%
if (scripts) {
  scripts.forEach(function (script) {
    %>
    <script src="/scripts/<%= script %>.js?v=<%= release %>"></script>
    <%
  })
}
if (stylesheets) {
  stylesheets.forEach(function (stylesheet) {
    %>
    <link rel="stylesheet" href="/stylesheets/<%= stylesheet %>.css?v=<%= release %>" type="text/css">
    <%
  })
}

%>
